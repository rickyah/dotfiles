# default path
PATH=/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin

# node 
PATH=$PATH:$(npm config get prefix)/bin

# homebrew
PATH=$(brew --prefix)/bin:$(brew --prefix)/sbin:$(brew --prefix coreutils)/libexec/gnubin:$PATH

# Add `~/bin` to the `$PATH`
PATH=$HOME/bin:$PATH

# If there is bin folder in the current location, it takes preference
# Usefull for binstubs in rails
PATH=./bin:$PATH

export PATH