// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada.
{
    "editor.fontFamily": "JetBrains Mono,Fira Code,Menlo, Monaco,'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.wrappingIndent": "indent",
    "editor.rulers": [
        80,
        100
    ],
    "files.exclude": {
        "**/*.meta": true
    },
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "workbench.colorTheme": "Gruvbox Dark (Hard)",
    "window.titleBarStyle": "native",
    "editor.renderIndentGuides": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.tips.enabled": false,
    "explorer.openEditors.visible": 0,
    "editor.formatOnSave": false,
    "editor.multiCursorModifier": "alt",
    "editor.renderControlCharacters": true,
    "csharp.suppressDotnetInstallWarning": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.snippetSuggestions": "top",
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#BBBF"
    },
    "code-eol.color": "#666",
    "python.pythonPath": "python3",
    "extensions.ignoreRecommendations": false,
    "debug.toolBarLocation": "docked",
    "diffEditor.ignoreTrimWhitespace": true,
    "gitlens.codeLens.scopes": [
        "blocks",
        "containers"
    ],
    "gitlens.blame.toggleMode": "window",
    "gitlens.settings.mode": "advanced",
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.menus": {
        "editor": {
            "blame": true,
            "clipboard": true,
            "compare": true,
            "details": true,
            "history": true,
            "remote": true
        },
        "editorGroup": {
            "blame": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "editorTab": {
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "compare": true,
            "history": true,
            "remote": true
        }
    },
    "java.errors.incompleteClasspath.severity": "ignore",
    "gitlens.resultsExplorer.files.layout": "tree",
    "sync.gist": "2c51f742d871a517738872a48b5e2fe0",
    "sync.quietSync": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "gitlens.advanced.telemetry.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "csharp.format.enable": false,
    "omnisharp.useEditorFormattingSettings": false,
    "editor.cursorBlinking": "smooth",
    "workbench.editor.highlightModifiedTabs": true,
    "editor.smoothScrolling": true,
    "editor.renderLineHighlight": "all",
    "editor.cursorWidth": 3,
    "editor.find.autoFindInSelection": "multiline",
    "editor.minimap.showSlider": "always",
    "workbench.fontAliasing": "auto",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.editor.tabCloseButton": "left",
    "workbench.editor.tabSizing": "shrink",
    "window.nativeTabs": true,
    "window.title": "${activeEditorShort}${separator}${rootName}${dirty}",
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.showInStatusBar": "never",
    "editor.minimap.enabled": false,
    "diffEditor.renderSideBySide": false,
    "editor.showFoldingControls": "always",
    "gitlens.currentLine.scrollable": false,
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "workbench.sideBar.location": "right",
    "search.enableSearchEditorPreview": true,
    "search.showLineNumbers": true,
    "search.searchEditorPreview.doubleClickBehaviour": "openLocationToSide",
    "jenkins.pipeline.linter.connector.url": "http://localhost:8080/pipeline-model-converter/validate",
    "jenkins.pipeline.linter.connector.user": "admin",
    "jenkins.pipeline.linter.connector.pass": "admin",
    "razor.format.enable": false,
    "editor.trimAutoWhitespace": false,
    "editor.columnSelection": false,
    "editor.minimap.size": "fill",
    "editor.autoIndent": "brackets",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "todo-tree.tree.showScanModeButton": false,
    "git-graph.maxDepthOfRepoSearch": 1,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "cSpell.enabled": false,
    "githubPullRequests.remotes": [
        "origin"
    ],
    "editor.largeFileOptimizations": false,
    "omnisharp.path": "latest",
    "omnisharp.enableDecompilationSupport": true,
    "csharp.semanticHighlighting.enabled": true,
    "razor.disabled": true,
    "git-graph.repository.commits.fetchAvatars": true,
    "scm.alwaysShowRepositories": true,
    "scm.repositories.visible": 0,
    "git.autoRepositoryDetection": false,
    "search.mode": "reuseEditor",
"foam.files.ignore": [],
"markdown.preview.doubleClickToSwitchToEditor": false,
"markdown.preview.scrollPreviewWithEditor": false,
"git.confirmSync": false,
"gitdoc.filePattern": "**/*.md",
"gitdoc.enabled": true,
"todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
],
"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
"workbench.list.smoothScrolling": true,
"markdown.preview.scrollEditorWithPreview": false,
"gitlens.views.commits.avatars": false,
"gitlens.views.repositories.includeWorkingTree": true,
"gitlens.blame.heatmap.enabled": false,
"gitlens.blame.compact": false,
"gitlens.changes.toggleMode": "window",
"gitlens.heatmap.locations": [
    "overview",
    "gutter"
],
"gitdoc.autoCommitDelay": 60,
"githubPullRequests.telemetry.enabled": false
}